    <FNPRegexs>
      <Regex
        Enabled="true"
        RE="(^|[^a-z])s?(?&lt;s&gt;[0-9]+)[ex](?&lt;e&gt;[0-9]{2,})[^a-z]"
        UseFullPath="false"
        Notes="3x23 s3x23 3e23 s3e23" />
      <Regex
        Enabled="false"
        RE="(^|[^a-z])s?(?&lt;s&gt;[0-9]+)(?&lt;e&gt;[0-9]{2,})[^a-z]"
        UseFullPath="false"
        Notes="323 or s323 for season 3, episode 23. 2004 for season 20, episode 4." />
      <Regex
        Enabled="false"
        RE="(^|[^a-z])s(?&lt;s&gt;[0-9]+)--e(?&lt;e&gt;[0-9]{2,})[^a-z]"
        UseFullPath="false"
        Notes="S02--E03" />
      <Regex
        Enabled="false"
        RE="(^|[^a-z])s(?&lt;s&gt;[0-9]+) e(?&lt;e&gt;[0-9]{2,})[^a-z]"
        UseFullPath="false"
        Notes="'S02.E04' and 'S02 E04'" />
      <Regex
        Enabled="false"
        RE="^(?&lt;s&gt;[0-9]+) (?&lt;e&gt;[0-9]{2,})"
        UseFullPath="false"
        Notes="filenames starting with '1.23' for season 1, episode 23" />
      <Regex
        Enabled="true"
        RE="(^|[^a-z])(?&lt;s&gt;[0-9])(?&lt;e&gt;[0-9]{2,})[^a-z]"
        UseFullPath="false"
        Notes="Show - 323 - Foo" />
      <Regex
        Enabled="true"
        RE="(^|[^a-z])se(?&lt;s&gt;[0-9]+)([ex]|ep|xep)?(?&lt;e&gt;[0-9]+)[^a-z]"
        UseFullPath="false"
        Notes="se3e23 se323 se1ep1 se01xep01..." />
      <Regex
        Enabled="true"
        RE="(^|[^a-z])(?&lt;s&gt;[0-9]+)-(?&lt;e&gt;[0-9]{2,})[^a-z]"
        UseFullPath="false"
        Notes="3-23 EpName" />
      <Regex
        Enabled="true"
        RE="(^|[^a-z])s(?&lt;s&gt;[0-9]+) +- +e(?&lt;e&gt;[0-9]{2,})[^a-z]"
        UseFullPath="false"
        Notes="ShowName - S01 - E01" />
      <Regex
        Enabled="true"
        RE="\b(?&lt;e&gt;[0-9]{2,}) ?- ?.* ?- ?(?&lt;s&gt;[0-9]+)"
        UseFullPath="false"
        Notes="like '13 - Showname - 2 - Episode Title.avi'" />
      <Regex
        Enabled="true"
        RE="\b(episode|ep|e) ?(?&lt;e&gt;[0-9]{2,}) ?- ?(series|season) ?(?&lt;s&gt;[0-9]+)"
        UseFullPath="false"
        Notes="episode 3 - season 4" />
      <Regex
        Enabled="true"
        RE="season (?&lt;s&gt;[0-9]+)\\e?(?&lt;e&gt;[0-9]{1,3}) ?-"
        UseFullPath="true"
        Notes="Show Season 3\E23 - Epname" />
      <Regex
        Enabled="false"
        RE="season (?&lt;s&gt;[0-9]+)\\episode (?&lt;e&gt;[0-9]{1,3})"
        UseFullPath="true"
        Notes="Season 3\Episode 23" />
    </FNPRegexs>
